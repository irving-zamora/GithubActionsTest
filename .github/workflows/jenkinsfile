pipeline {
    agent any
    environment {
        GO_VERSION = '1.20' 
        TYK_VERSION = 'v5.0.3'
    }
    tools {
        go '1.20'
    }

    stages {
        stage('Clean Up') {
            steps {
                deleteDir()
                echo "cleaned"
            }
        }
        stage('Checkout code') {
            steps {
                script {
                    sh 'git clone https://github.com/irving-zamora/GithubActionsTest.git'
                }
            }
        }
    }
}